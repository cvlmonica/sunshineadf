{
	"name": "Copy from Profisee REST API to CSV",
	"properties": {
		"activities": [
			{
				"name": "Copy from Profisee REST API to CSV",
				"description": "Copy records from Profisee via the Profisee REST API to ADLS Gen2 in CSV format.",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "RestSource",
						"httpRequestTimeout": "00:01:40",
						"requestInterval": "00.00:00:00.010",
						"requestMethod": "GET",
						"additionalHeaders": {
							"x-api-key": "59ef40dbd3974ea7917514edc55ea026",
							"content-type": "application/json; charset=utf-8"
						},
						"paginationRules": {
							"AbsoluteUrl": "$.nextPage"
						}
					},
					"sink": {
						"type": "JsonSink",
						"storeSettings": {
							"type": "AzureBlobStorageWriteSettings"
						},
						"formatSettings": {
							"type": "JsonWriteSettings",
							"filePattern": "setOfObjects"
						}
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"mappings": [
							{
								"source": {
									"path": "$['data'][0]['internalId']"
								},
								"sink": {
									"name": "internalId"
								}
							},
							{
								"source": {
									"path": "$['data'][0]['id']"
								},
								"sink": {
									"name": "id"
								}
							},
							{
								"source": {
									"path": "$['data'][0]['code']"
								},
								"sink": {
									"name": "code"
								}
							},
							{
								"source": {
									"path": "$['data'][0]['name']"
								},
								"sink": {
									"name": "name"
								}
							},
							{
								"source": {
									"path": "$['data'][0]['first Name']"
								},
								"sink": {
									"name": "first Name"
								}
							},
							{
								"source": {
									"path": "$['data'][0]['last Name']"
								},
								"sink": {
									"name": "last Name"
								}
							},
							{
								"source": {
									"path": "$['data'][0]['tax ID']"
								},
								"sink": {
									"name": "tax ID"
								}
							}
						],
						"collectionReference": "$['data']"
					}
				},
				"inputs": [
					{
						"referenceName": "ProfiseeRESTAPI_Records_Get",
						"type": "DatasetReference",
						"parameters": {
							"entityId": {
								"value": "@variables('EntityId')",
								"type": "Expression"
							},
							"pageSize": 1000,
							"attributes": " ",
							"filter": " ",
							"orderBy": " ",
							"dbaFormat": 1,
							"recordCodes": " "
						}
					}
				],
				"outputs": [
					{
						"referenceName": "Json1",
						"type": "DatasetReference",
						"parameters": {
							"filename": {
								"value": "@variables('EntityId')",
								"type": "Expression"
							}
						}
					}
				]
			}
		],
		"variables": {
			"OutputContainer": {
				"type": "String",
				"defaultValue": "datasource1"
			},
			"EntityId": {
				"type": "String",
				"defaultValue": "Customer"
			}
		},
		"folder": {
			"name": "CSV Format"
		},
		"annotations": [],
		"lastPublishTime": "2020-09-25T15:29:08Z"
	}
}