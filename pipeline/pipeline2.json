{
	"name": "pipeline2",
	"properties": {
		"activities": [
			{
				"name": "Copy data1",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "JsonSource",
						"storeSettings": {
							"type": "AzureBlobStorageReadSettings",
							"recursive": true,
							"enablePartitionDiscovery": false
						},
						"formatSettings": {
							"type": "JsonReadSettings"
						}
					},
					"sink": {
						"type": "AzureSqlSink",
						"writeBehavior": "insert",
						"sqlWriterUseTableLock": false,
						"tableOption": "autoCreate",
						"disableMetricsCollection": false
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"mappings": [
							{
								"source": {
									"path": "$['pageNumber']"
								}
							},
							{
								"source": {
									"path": "$['pageSize']"
								}
							},
							{
								"source": {
									"path": "$['resultCount']"
								}
							},
							{
								"source": {
									"path": "$['totalPages']"
								}
							},
							{
								"source": {
									"path": "$['previousPage']"
								}
							},
							{
								"source": {
									"path": "$['nextPage']"
								}
							},
							{
								"source": {
									"path": "$['totalRecords']"
								}
							},
							{
								"source": {
									"path": "['internalId']"
								},
								"sink": {
									"name": "internalId",
									"type": "Int64"
								}
							},
							{
								"source": {
									"path": "['id']"
								},
								"sink": {
									"name": "id",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "['code']"
								},
								"sink": {
									"name": "code",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "['name']"
								},
								"sink": {
									"name": "name",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "['first Name']"
								},
								"sink": {
									"name": "first Name",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "['last Name']"
								},
								"sink": {
									"name": "last Name",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "['tax ID']"
								}
							},
							{
								"source": {
									"path": "['category']"
								}
							},
							{
								"source": {
									"path": "['parent Customer']"
								}
							},
							{
								"source": {
									"path": "['model Confidence']"
								}
							},
							{
								"source": {
									"path": "['record Source']"
								}
							},
							{
								"source": {
									"path": "['match Status']"
								}
							},
							{
								"source": {
									"path": "['match Cluster']"
								}
							},
							{
								"source": {
									"path": "['match Member']"
								}
							},
							{
								"source": {
									"path": "['match Strategy']"
								}
							},
							{
								"source": {
									"path": "['match DateTime']"
								}
							},
							{
								"source": {
									"path": "['match MultiGroup']"
								}
							},
							{
								"source": {
									"path": "['match User']"
								}
							},
							{
								"source": {
									"path": "['golden Record']"
								}
							},
							{
								"source": {
									"path": "['proposed Count']"
								}
							},
							{
								"source": {
									"path": "['approved Count']"
								}
							},
							{
								"source": {
									"path": "['enterDTM']"
								}
							},
							{
								"source": {
									"path": "['enterUserName']"
								}
							},
							{
								"source": {
									"path": "['lastChgDTM']"
								}
							},
							{
								"source": {
									"path": "['lastChgUserName']"
								}
							},
							{
								"source": {
									"path": "['validationStatusID']"
								}
							},
							{
								"source": {
									"path": "['canUpdate']"
								}
							},
							{
								"source": {
									"path": "['canDelete']"
								}
							}
						],
						"collectionReference": "$['data']"
					}
				},
				"inputs": [
					{
						"referenceName": "Json1",
						"type": "DatasetReference",
						"parameters": {
							"filename": "Customer"
						}
					}
				],
				"outputs": [
					{
						"referenceName": "AzureSqlTable1",
						"type": "DatasetReference"
					}
				]
			}
		],
		"folder": {
			"name": "CSV Format"
		},
		"annotations": []
	}
}